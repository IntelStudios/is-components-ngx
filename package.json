{
  "name": "is-components-ngx",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "node build-local-package.js",
    "build.local": "node local-deps.js",
    "build.prod": "node local-deps.js && ng build --prod",
    "publish.select": "ng build --prod is-select && npm publish ./dist/is-select",
    "test.select": "ng test is-select --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "publish.select-tree": "ng build --prod is-select-tree && npm publish ./dist/is-select-tree",
    "publish.dx-select-tree": "ng build --prod is-dx-select-tree && npm publish ./dist/is-dx-select-tree",
    "publish.input-mapping": "ng build --prod is-input-mapping && npm publish ./dist/is-input-mapping",
    "publish.codemirror": "ng build --prod is-codemirror && npm publish ./dist/is-codemirror",
    "publish.froala": "ng build --prod is-froala && npm publish ./dist/is-froala",
    "publish.core-ui": "ng build --prod is-core-ui && node copy-styles.js && npm publish ./dist/is-core-ui",
    "publish.datepicker": "ng build --prod is-datepicker && npm publish ./dist/is-datepicker",
    "publish.bootstrap-switch": "ng build --prod is-bootstrap-switch && npm publish ./dist/is-bootstrap-switch",
    "publish.modal": "ng build --prod is-modal && npm publish ./dist/is-modal",
    "publish.grid": "ng build --prod is-grid && npm publish ./dist/is-grid",
    "publish.table": "ng build --prod is-table && npm publish ./dist/is-table",
    "publish.timepicker": "ng build --prod is-timepicker && npm publish ./dist/is-timepicker",
    "publish.cron": "ng build is-cron-editor && npm publish ./dist/is-cron-editor",
    "publish.editable-textbox": "ng build --prod is-editable-textbox && npm publish ./dist/is-editable-textbox",
    "publish.checkmap": "ng build --prod is-checkmap && npm publish ./dist/is-checkmap",
    "publish.cdk": "ng build --prod is-cdk && npm publish ./dist/is-cdk",
    "docs": "./node_modules/.bin/compodoc -p tsconfig.json -d dist/app/docs --silent --theme readthedocs --disableCoverage --disableGraph --disablePrivate",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^11.2.14",
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@circlon/angular-tree-component": "^11.0.4",
    "@intelstudios/font-awesome": "^6.1.1-1",
    "@intelstudios/text-utils": "^1.1.2",
    "@ngx-translate/core": "^13.0.0",
    "at.js": "^1.5.4",
    "bootstrap": "^4.3.1",
    "codemirror": "^5.43.0",
    "core-js": "^2.6.5",
    "froala-editor": "^2.9.5",
    "install": "^0.13.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "ng-modal-lib": "0.0.8",
    "ngx-bootstrap": "^7.1.0",
    "ngx-mask": "^11.1.5",
    "npm": "^8.1.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@angular/language-service": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/cli": "~11.2.14",
    "@angular-devkit/build-angular": "~0.1102.13",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "^4.0.2"
  },
  "volta": {
    "node": "14.18.3"
  }
}
