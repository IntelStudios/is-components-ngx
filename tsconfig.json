{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "is-select": [
        "dist/is-select"
      ],
      "is-select/*": [
        "dist/is-select/*"
      ],
      "is-codemirror": [
        "dist/is-codemirror"
      ],
      "is-codemirror/*": [
        "dist/is-codemirror/*"
      ],
      "is-froala": [
        "dist/is-froala"
      ],
      "is-froala/*": [
        "dist/is-froala/*"
      ],
      "is-metronic": [
        "dist/is-metronic"
      ],
      "is-metronic/*": [
        "dist/is-metronic/*"
      ],
      "is-datepicker": [
        "dist/is-datepicker"
      ],
      "is-datepicker/*": [
        "dist/is-datepicker/*"
      ],
      "is-bootstrap-switch": [
        "dist/is-bootstrap-switch"
      ],
      "is-bootstrap-switch/*": [
        "dist/is-bootstrap-switch/*"
      ],
      "is-select-tree": [
        "dist/is-select-tree"
      ],
      "is-select-tree/*": [
        "dist/is-select-tree/*"
      ],
      "is-modal": [
        "dist/is-modal"
      ],
      "is-modal/*": [
        "dist/is-modal/*"
      ],
      "is-grid": [
        "dist/is-grid"
      ],
      "is-grid/*": [
        "dist/is-grid/*"
      ],
      "is-timepicker": [
        "dist/is-timepicker"
      ],
      "is-cron-editor": [
        "dist/is-cron-editor"
      ],
      "is-cron-editor/*": [
        "dist/is-cron-editor/*"
      ],
      "is-checkmap": [
        "dist/is-checkmap"
      ],
      "is-checkmap/*": [
        "dist/is-checkmap/*"
      ],
      "is-input-mapping": [
        "dist/is-input-mapping"
      ],
      "is-input-mapping/*": [
        "dist/is-input-mapping/*"
      ],
      "is-editable-textbox": [
        "dist/is-editable-textbox"
      ],
      "is-editable-textbox/*": [
        "dist/is-editable-textbox/*"
      ]
    }
  }
}