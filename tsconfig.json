{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@intelstudios/select": [
        "dist/is-select"
      ],
      "@intelstudios/select/*": [
        "dist/is-select/*"
      ],
      "@intelstudios/codemirror": [
        "dist/is-codemirror"
      ],
      "@intelstudios/codemirror/*": [
        "dist/is-codemirror/*"
      ],
      "@intelstudios/froala": [
        "dist/is-froala"
      ],
      "@intelstudios/froala/*": [
        "dist/is-froala/*"
      ],
      "@intelstudios/core-ui": [
        "dist/is-core-ui"
      ],
      "@intelstudios/core-ui/*": [
        "dist/is-core-ui/*"
      ],
      "@intelstudios/datepicker": [
        "dist/is-datepicker"
      ],
      "@intelstudios/datepicker/*": [
        "dist/is-datepicker/*"
      ],
      "@intelstudios/bootstrap-switch": [
        "dist/is-bootstrap-switch"
      ],
      "@intelstudios/bootstrap-switch/*": [
        "dist/is-bootstrap-switch/*"
      ],
      "@intelstudios/select-tree": [
        "dist/is-select-tree"
      ],
      "@intelstudios/select-tree/*": [
        "dist/is-select-tree/*"
      ],
      "@intelstudios/modal": [
        "dist/is-modal"
      ],
      "@intelstudios/modal/*": [
        "dist/is-modal/*"
      ],
      "@intelstudios/grid": [
        "dist/is-grid"
      ],
      "@intelstudios/grid/*": [
        "dist/is-grid/*"
      ],
      "@intelstudios/timepicker": [
        "dist/is-timepicker"
      ],
      "@intelstudios/cron-editor": [
        "dist/is-cron-editor"
      ],
      "@intelstudios/cron-editor/*": [
        "dist/is-cron-editor/*"
      ],
      "is-checkmap": [
        "dist/is-checkmap"
      ],
      "@intelstudios/checkmap/*": [
        "dist/is-checkmap/*"
      ],
      "@intelstudios/input-mapping": [
        "dist/is-input-mapping"
      ],
      "@intelstudios/input-mapping/*": [
        "dist/is-input-mapping/*"
      ],
      "@intelstudios/editable-textbox": [
        "dist/is-editable-textbox"
      ],
      "@intelstudios/editable-textbox/*": [
        "dist/is-editable-textbox/*"
      ],
      "@intelstudios/cdk": [
        "dist/is-cdk/is-cdk",
        "dist/is-cdk"
      ]
    }
  }
}